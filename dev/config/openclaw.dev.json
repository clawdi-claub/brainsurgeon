{
  "logging": {
    "level": "debug"
  },
  "gateway": {
    "port": 18789,
    "mode": "local",
    "bind": "lan",
    "auth": {
      "mode": "token",
      "token": "dev_gateway_token_please_change"
    }
  },
  "plugins": {
    "allow": ["brainsurgeon"],
    "entries": {
      "brainsurgeon": {
        "enabled": true,
        "config": {
          "agentsDir": "/root/.openclaw/agents",
          "apiUrl": "http://brainsurgeon-api-dev:8000",
          "enableAutoPrune": false,
          "autoPruneThreshold": 3,
          "keepRestoreRemoteCalls": false,
          "busDbPath": "/root/.openclaw/brainsurgeon/bus.db"
        }
      }
    }
  },
  "agents": {
    "defaults": {
      "model": {
        "primary": "ollama/kimi-k2.5:cloud"
      }
    }
  }
}
